{
	"name": "client-project-dgfc",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"client": "webpack-dev-server --config build-utils/webpack.config.js --env.env=dev",
		"server": "nodemon server/index.js",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"build": "webpack --config build-utils/webpack.config.js --env.env=prod",
		"build:analyze": "npm run build -- --env.addon=bundleanalyze --env.addon=bundlevisualizer",
		"start": "npm run build && node server/index.js",
		"client-lint": "eslint ./client/src/",
		"server-lint": "eslint ./server/",
		"eslint-check": "eslint --print-config client/src/App.js | eslint-config-prettier-check",
		"create-migration": "cd ./server && npx db-migrate create",
		"migrate": "cd ./server && npx db-migrate up",
		"test-client": "jest ./client/src",
		"test-server": "jest --runInBand ./server/tests"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"repository": {
		"type": "git",
		"url": "git@gitlab.cs.cf.ac.uk:c1662348/project7-danau-girang.git"
	},
